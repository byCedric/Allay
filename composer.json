{
    "name": "bycedric/allay",
    "type": "library",
    "description": "Build scalable and restful API's, in minutes.",
    "keywords": [
        "allay",
        "laravel",
        "bycedric",
        "api"
    ],
    "homepage": "http://github.com/bycedric/allay",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric van Putten",
            "email": "me@bycedric.com",
            "homepage": "http://bycedric.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/contracts": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/http": "^5.1",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "illuminate/routing": "^5.1",
        "laravel/lumen-framework": "^5.1",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^5.0",
        "satooshi/php-coveralls": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "ByCedric\\Allay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ByCedric\\Allay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
